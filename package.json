{
  "name": "arbitrage",
  "version": "1.0.0",
  "main": "index.js",
  "types": "module",
  "license": "MIT",
  "scripts": {
    "postinstall": "yarn patch-package",
    "build": "tsc",
    "start:testnet": "tsc && node --enable-source-maps -r dotenv/config ./build/src/index.js dotenv_config_path=.env.testnet | pino-pretty -t",
    "start:mainnet": "tsc && node --enable-source-maps -r dotenv/config ./build/src/index.js dotenv_config_path=./.env.mainnet | pino-pretty -t",
    "startWithEnvs:mainnet": "tsc && node --enable-source-maps ./build/src/index.js | pino-pretty -t",
    "deploy:goerli": "yarn hardhat deploy --network goerli",
    "console:goerli": "yarn hardhat console --network goerli",
    "fork:console": "HARDHAT_FORK=goerli yarn hardhat console",
    "fork:deploy": "HARDHAT_FORK=goerli yarn hardhat deploy",
    "lint": "eslint . --ext .ts,.tsx --fix"
  },
  "dependencies": {
    "@ethersproject/abstract-provider": "^5.6.0",
    "@flashbots/ethers-provider-bundle": "^0.4.3",
    "@nomiclabs/hardhat-etherscan": "^3.0.3",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@openzeppelin/contracts": "^4.6.0",
    "@uniswap/sdk-core": "^3.0.1",
    "@uniswap/swap-router-contracts": "^1.2.1",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "@uniswap/v2-sdk": "^3.0.1",
    "@uniswap/v3-periphery": "^1.4.1",
    "@uniswap/v3-sdk": "^3.8.2",
    "bignumber.js": "^9.0.2",
    "chai": "^4.3.6",
    "convict": "^6.2.3",
    "dotenv": "^16.0.0",
    "ethereum-waffle": "^3.4.4",
    "ethers": "^5.6.4",
    "express": "^4.18.1",
    "hardhat": "^2.9.3",
    "jsbi": "^3.1.4",
    "module-alias": "^2.2.2",
    "multicoin-address-validator": "^0.5.10",
    "node-fetch": "^2.6.6",
    "pino": "^8.0.0",
    "pino-pretty": "^8.0.0",
    "tiny-invariant": "^1.2.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers",
    "@types/convict": "^6.1.1",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.1.1",
    "@types/multicoin-address-validator": "^0.5.0",
    "@types/node": "16.4.2",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-standard": "^5.0.0",
    "hardhat-deploy": "^0.11.4",
    "prettier": "^2.6.2",
    "prettier-plugin-solidity": "^1.0.0-beta.19"
  },
  "_moduleAliases": {
    "~balancer": "./build/src/balancer",
    "~balancer/*": "./build/src/balancer/*",
    "~fetcher": "./build/src/fetcher",
    "~fetcher/*": "./build/src/fetcher/*",
    "~interfaces": "./build/src/interfaces",
    "~interfaces/*": "./build/src/interfaces/*",
    "~transactions": "./build/src/transactions",
    "~transactions/*": "./build/src/transactions/*",
    "~utils": "./build/src/utils",
    "~utils/*": "./build/src/utils/*",
    "~config": "./build/src/config",
    "~config/*": "./build/src/config/*"
  }
}
